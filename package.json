{
  "name": "@fsegurai/codemirror-themes",
  "version": "6.25.1",
  "description": "Theme packages for the CodeMirror editor",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Fabi√°n Segura",
    "url": "https://www.fsegurai.com/"
  },
  "keywords": [
    "code",
    "codemirror",
    "codemirror-6",
    "diff",
    "diff-editor",
    "editor",
    "highlight",
    "javascript",
    "javascript-library",
    "library",
    "syntax",
    "typescript",
    "typescript-library",
    "themes",
    "abcdef",
    "abyss",
    "android-studio",
    "andromeda",
    "basic-dark",
    "basic-light",
    "bundle",
    "cobalt2",
    "forest",
    "github-dark",
    "github-light",
    "gruvbox-dark",
    "gruvbox-light",
    "material-dark",
    "material-light",
    "monokai",
    "nord",
    "palenight",
    "solarized-dark",
    "solarized-light",
    "tokyo-night-day",
    "tokyo-night-storm",
    "volcano",
    "vscode-dark",
    "vscode-light"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean:demo": "rimraf --glob ./demo/dist && rimraf --glob ./demo/README.md && rimraf --glob ./demo/LICENSE",
    "clean:packages": "rimraf --glob ./packages/*/dist",
    "clean:all": "bun run clean:demo && bun run clean:packages",
    "start": "rollup -w -c rollup.config.js",
    "start:doppler": "doppler run bun run start",
    "copy:helpers": "bun ./helper/copy-helpers.ts",
    "build:demo": "rollup -c rollup.config.js",
    "build_post:demo": "cpy ./README.md ./demo && cpy ./LICENSE ./demo",
    "build_doppler:demo": "doppler run bun run build:demo && bun run build_post:demo",
    "build:packages": "bun run --filter '*' prepare",
    "build:all": "bun run build:demo && bun run build:packages",
    "lint": "bun run lint:demo && bun run lint:packages",
    "lint:demo": "eslint ./demo",
    "lint:packages": "eslint ./packages/*/src/**"
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/language": "^6.11.3",
    "@codemirror/language-data": "^6.5.2",
    "@codemirror/legacy-modes": "^6.5.2",
    "@codemirror/merge": "^6.11.2",
    "@lezer/markdown": "^1.6.0",
    "@material/web": "^2.4.1",
    "clipboard": "^2.0.11",
    "codemirror": "^6.0.2",
    "marked": "^17.0.1",
    "marked-highlight": "^2.2.3",
    "prismjs": "^1.30.0"
  },
  "devDependencies": {
    "@codemirror/buildhelper": "^1.0.2",
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/prismjs": "^1.26.5",
    "cpy-cli": "^6.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.3",
    "rollup-plugin-dev": "^2.1.0",
    "semantic-release": "^25.0.2",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.47.0"
  }
}
